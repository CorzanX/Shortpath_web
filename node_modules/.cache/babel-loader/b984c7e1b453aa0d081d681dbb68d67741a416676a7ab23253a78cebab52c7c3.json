{"ast":null,"code":"import axios from 'axios';\nimport undirected_graph from './undirected_graph.vue';\nexport default {\n  components: {\n    undirected_graph\n  },\n  data() {\n    return {\n      txtData: '',\n      nodes: [],\n      links: []\n    };\n  },\n  methods: {\n    beforeUpload(file) {\n      // 只接受txt文件\n      const isTxt = file.type === 'text/plain';\n      if (!isTxt) {\n        this.$message.error('只能上传txt文件');\n      }\n      return isTxt;\n    },\n    handleSuccess(response, file) {\n      console.log(response);\n      const graphData = response;\n      console.log(graphData);\n      console.log(graphData.links.map(link => ({\n        source: link.source,\n        target: link.target,\n        weight: link.weight\n      })));\n      if (!graphData) {\n        // 处理 graphData 不存在的情况，比如给 nodes 和 links 赋一个空数组\n        this.nodes = [];\n        this.links = [];\n        return;\n      }\n      this.nodes = graphData.nodes.map(node => ({\n        id: node.id,\n        name: node.name\n      }));\n      this.links = graphData.links.map(link => ({\n        source: link.source,\n        target: link.target,\n        weight: link.weight\n      }));\n      this.txtData = response;\n      console.log(typeof this.nodes);\n    }\n  }\n};","map":{"version":3,"names":["axios","undirected_graph","components","data","txtData","nodes","links","methods","beforeUpload","file","isTxt","type","$message","error","handleSuccess","response","console","log","graphData","map","link","source","target","weight","node","id","name"],"sources":["src/components/Algcmp_stp.vue"],"sourcesContent":["<template>\n    <div>\n        <h1>example</h1>\n        <el-upload\n            class=\"upload-demo\"\n            action=\"http://localhost:8088/cmp\"\n            :on-success=\"handleSuccess\"\n            :before-upload=\"beforeUpload\"\n            >\n            <el-button size=\"small\" type=\"primary\">点击上传</el-button>\n            <div slot=\"tip\" class=\"el-upload__tip\">只能上传txt文件，且不超过500kb</div>\n        </el-upload>\n        <div v-if=\"txtData\">\n          \n          <h1>{{txtData}}</h1>\n        </div>\n    </div>\n</template>\n\n\n<script>\n  import axios from 'axios';\n  import undirected_graph from './undirected_graph.vue';\n  export default {\n    components:{\n        undirected_graph,\n      },\n    data() {\n      return {\n        txtData: '',\n        nodes: [],\n        links: []\n      };\n    },\n    methods: {\n      beforeUpload(file) {\n        // 只接受txt文件\n        const isTxt = file.type === 'text/plain';\n        if (!isTxt) {\n          this.$message.error('只能上传txt文件');\n        }\n        return isTxt;\n      },\n      handleSuccess(response,file) {\n        console.log(response);\n        const graphData = response;\n        console.log(graphData);\n        console.log(graphData.links.map(link => ({ source: link.source, target: link.target, weight: link.weight })))\n        if (!graphData) {\n          // 处理 graphData 不存在的情况，比如给 nodes 和 links 赋一个空数组\n          this.nodes = [];\n          this.links = [];\n          return;\n        }\n        this.nodes = graphData.nodes.map(node => ({ id: node.id, name: node.name }));\n        this.links = graphData.links.map(link => ({ source: link.source, target: link.target, weight: link.weight }));\n        this.txtData = response;\n        console.log(typeof this.nodes);\n      }\n    }\n  }\n</script>"],"mappings":"AAqBA,OAAAA,KAAA;AACA,OAAAC,gBAAA;AACA;EACAC,UAAA;IACAD;EACA;EACAE,KAAA;IACA;MACAC,OAAA;MACAC,KAAA;MACAC,KAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAAC,IAAA;MACA;MACA,MAAAC,KAAA,GAAAD,IAAA,CAAAE,IAAA;MACA,KAAAD,KAAA;QACA,KAAAE,QAAA,CAAAC,KAAA;MACA;MACA,OAAAH,KAAA;IACA;IACAI,cAAAC,QAAA,EAAAN,IAAA;MACAO,OAAA,CAAAC,GAAA,CAAAF,QAAA;MACA,MAAAG,SAAA,GAAAH,QAAA;MACAC,OAAA,CAAAC,GAAA,CAAAC,SAAA;MACAF,OAAA,CAAAC,GAAA,CAAAC,SAAA,CAAAZ,KAAA,CAAAa,GAAA,CAAAC,IAAA;QAAAC,MAAA,EAAAD,IAAA,CAAAC,MAAA;QAAAC,MAAA,EAAAF,IAAA,CAAAE,MAAA;QAAAC,MAAA,EAAAH,IAAA,CAAAG;MAAA;MACA,KAAAL,SAAA;QACA